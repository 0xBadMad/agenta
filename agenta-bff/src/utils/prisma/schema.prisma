// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

model LLMCall {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  prompt Json
  output String
  parameters Json?

  @@map("l_l_m_call")
}
